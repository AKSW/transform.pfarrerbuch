@prefix r2rml: <http://www.w3.org/ns/r2rml#> .
@prefix hp: <http://purl.org/voc/hp/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

###########################
# Position Mapping 1
###########################

<#PositionTableView1>
	r2rml:sqlQuery
		"""Select id, Berufung1_Als as position, Berufung1_Ort as address, Berufung1_Zeit as zeit from BioTabelle1;""".

<#PositionTriplesMap1>
	r2rml:logicalTable <#PositionTableView1>;
	r2rml:subjectMap [
		r2rml:template "http://pfarrerbuch.aksw.org/kps/position/{\"address\"}/{\"position\"}";
	];
	r2rml:predicateObjectMap [
			r2rml:predicate <http://www.w3.org/2000/01/rdf-schema#label>;
			r2rml:objectMap [
				r2rml:column "\"position\"";
				r2rml:termType r2rml:Literal
			];
	];
	r2rml:predicateObjectMap [
			r2rml:predicate hp:positionDesignation;
			r2rml:objectMap [
				r2rml:column "\"position\"";
				r2rml:termType r2rml:Literal
			];
	];
	r2rml:predicateObjectMap [
			r2rml:predicate hp:place;
			r2rml:objectMap [
				r2rml:template "http://pfarrerbuch.aksw.org/kps/position/{\"address\"}";
			];
	];
	r2rml:predicateObjectMap [
			r2rml:predicate hp:period;
			r2rml:objectMap [
				r2rml:template "http://pfarrerbuch.aksw.org/kps/position/{\"zeit\"}";
			];
	]
	.

###########################
# Position/Professor Mapping 1
###########################

<#PersonPositionTriplesMap1>
	r2rml:logicalTable <#PositionTableView1>;
	r2rml:subjectMap [
		r2rml:template "http://pfarrerbuch.aksw.org/kps/person/{\"id\"}";
	];
	r2rml:predicateObjectMap [
			r2rml:predicate hp:hasPosition;
			r2rml:objectMap [
				r2rml:template "http://pfarrerbuch.aksw.org/kps/position/{\"position\"}";
			];
	];
	.

